{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "cd .. && husky install client/.husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write **/*.{js,ts,jsx,tsx,md,json}",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "storycap --serverCmd \"storybook dev --ci -p 9001\" http://localhost:9001 --serverTimeout 100000 --captureTimeout 30000",
    "vrt": "reg-suit run",
    "generate": "plop --plopfile generators/index.js"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,jsx,tsx,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/node": "18.15.13",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@uiw/react-md-editor": "^3.20.8",
    "ajv": "^8.12.0",
    "autoprefixer": "10.4.14",
    "date-fns": "^2.29.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.2.4",
    "next-remove-imports": "^1.0.11",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-use-websocket": "^4.3.1",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "npm": "pnpm only",
    "yarn": "pnpm only"
  },
  "devDependencies": {
    "@next/env": "^13.3.1",
    "@storybook/addon-essentials": "7.0.8",
    "@storybook/addon-interactions": "7.0.8",
    "@storybook/addon-links": "7.0.8",
    "@storybook/blocks": "7.0.8",
    "@storybook/nextjs": "7.0.8",
    "@storybook/react": "7.0.8",
    "@storybook/testing-library": "0.0.14-next.2",
    "@tailwindcss/typography": "^0.5.9",
    "easymde": "^2.18.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.2",
    "puppeteer": "^20.0.0",
    "react-simplemde-editor": "^5.2.0",
    "reg-keygen-git-hash-plugin": "^0.12.1",
    "reg-notify-github-plugin": "^0.12.1",
    "reg-publish-gcs-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "storybook": "7.0.8",
    "storycap": "^4.1.1",
    "vitest": "^0.30.1"
  }
}
